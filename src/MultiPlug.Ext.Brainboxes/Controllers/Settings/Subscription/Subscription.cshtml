@model MultiPlug.Ext.Brainboxes.Models.Settings.Device.Subscription

<form action="" method="post" accept-charset="utf-8" enctype="application/x-www-form-urlencoded">
    <section class="row-fluid">

        <div class="row-fluid">
            <div class="box">
                <div class="span5">
                    <a href="/"><img alt="Brainboxes Logo" src="assets/brainboxes/images/brainboxes.png"></a>
                </div>
                <div class="span2">
                    <p style="font-size:22.75px; padding-top:10px;">Subscription</p>
                </div>
                <div class="span3">
                </div>
                <div class="span2">
                    <a class="btn btn-black" style="height:44px; width:35px" href="extensions/brainboxes/device/?id=@Model.DeviceId"><i class="icon-arrow-left icon-large"></i><p>Back</p></a>
                    <button id="" type="submit" value="submit" style="height:54px; width:65px" class="item btn btn-green"><i class="icon-save icon-large"></i><p>Save</p></button>
                </div>
            </div>
        </div>

        <input type="hidden" name="DeviceId" value="@Model.DeviceId">
        <input type="hidden" name="SubscriptionGuid" value="@Model.SubscriptionGuid">
        <input type="hidden" name="OutputId" value="@Model.OutputId">

        <div class="row-fluid">
            <h3 class="box-header">
                Subscription by Key and Value
            </h3>
            <div class="box non-collapsible">
                <div class="row-fluid">
                    <div class="span6">
                        <div class="row-fluid"><div class="span12">Subscription ID</div></div>
                        <div class="span12">
                            <input name="SubscriptionId" class="input-block-level mp-event-picker" type="text" value="@Model.SubscriptionId" />
                        </div>
                    </div>
                    <div class="span3">
                        <div class="row-fluid"><div class="span12">HIGH Key</div></div>
                        <div class="row-fluid"><div class="span12"><input name="HighKey" class="input-block-level" type="text" value="@Model.HighKey"></div></div>
                    </div>
                    <div class="span3">
                        <div class="row-fluid"><div class="span12">HIGH Value</div></div>
                        <div class="row-fluid"><div class="span12"><input name="HighValue" class="input-block-level" type="text" value="@Model.HighValue"></div></div>
                    </div>
                </div>

                <div class="row-fluid">
                    <div class="span6">
                    </div>
                    <div class="span3">
                        <div class="row-fluid"><div class="span12">LOW Key</div></div>
                        <div class="row-fluid"><div class="span12"><input name="LowKey" class="input-block-level" type="text" value="@Model.LowKey"></div></div>
                    </div>
                    <div class="span3">
                        <div class="row-fluid"><div class="span12">LOW Value</div></div>
                        <div class="row-fluid"><div class="span12"><input name="LowValue" class="input-block-level" type="text" value="@Model.LowValue"></div></div>
                    </div>
                </div>
            </div>
        </div>

    </section>
</form>
